<script>
    const tabs = document.querySelectorAll('.tab');
    const twowhelar = document.getElementById('twowhelar');
    const homeappliances = document.getElementById('homeappliances');
    const fourwheeler = document.getElementById('brand-list');
    tabs.forEach((tab, index) => {
        tab.addEventListener('click', () => {
            tabs.forEach(tab => tab.classList.remove('active'));
            if (index === 0) {
                twowhelar.style.display = 'block';
                homeappliances.style.display = 'none';
                fourwheeler.style.display = 'none';
            } else if (index === 1) {
                twowhelar.style.display = 'none';
                homeappliances.style.display = 'none';
                fourwheeler.style.display = 'block';
            } else {
                twowhelar.style.display = 'none';
                homeappliances.style.display = 'block';
                fourwheeler.style.display = 'none';
            }
            tab.classList.add('active');
        });
    });
</script>
